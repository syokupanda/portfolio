<div class="container" >
  <h1>Admins::Accounts#index</h1>
  <table class='table table-hover table-inverse'>
    <thead>
      <tr>
        <th>ニックネーム</th>
        <th>メールアドレス</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= link_to admins_account_path(user) do %>
              <%= user.nickname %>
            <% end %>
          </td>
          <td>
            <%= user.email %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
